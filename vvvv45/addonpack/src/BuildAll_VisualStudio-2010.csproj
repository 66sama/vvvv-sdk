  <Target Name="Build" DependsOnTargets="$(BuildDependsOn)">
    <MSBuild Projects="@(CoreProjectsToBuild)" ContinueOnError="false" Properties="Configuration=$(Configuration);Platform=AnyCPU" />
	  <MSBuild Projects="@(CorePluginsToBuild)" ContinueOnError="false" Properties="Configuration=$(Configuration);Platform=AnyCPU" >
		  <Output TaskParameter="TargetOutputs" ItemName="CorePluginsOutputFiles" />
    </MSBuild>
	  <MSBuild Projects="@(CoreEditorsToBuild)" ContinueOnError="false" Properties="Configuration=$(Configuration);Platform=AnyCPU" >
		  <Output TaskParameter="TargetOutputs" ItemName="CoreEditorsOutputFiles" />
    </MSBuild>
    <MSBuild Projects="@(PluginProjectsToBuild)" ContinueOnError="false" Properties="Configuration=$(Configuration);Platform=AnyCPU" Condition="!Exists('%(PluginProjectsToBuild.RootDir)%(PluginProjectsToBuild.Directory)ignore')">
      <Output TaskParameter="TargetOutputs" ItemName="PluginsOutputFiles" />
    </MSBuild>
    <CreateItem Include="core\**\bin\$(Configuration)\*;gui\**\bin\$(Configuration)\*">
      <Output TaskParameter="Include" ItemName="CoreOutputFiles" />
    </CreateItem>
    <!-- Make sure *.pdb files are included -->
    <CreateItem Include="@(PluginsOutputFiles->'%(RootDir)%(Directory)%(Filename).*')">
      <Output TaskParameter="Include" ItemName="PluginsOutputFiles" />
    </CreateItem>
	  <CreateItem Include="@(CorePluginsOutputFiles->'%(RootDir)%(Directory)%(Filename).*')">
      <Output TaskParameter="Include" ItemName="CorePluginsOutputFiles" />
    </CreateItem>
    <CreateItem Include="@(CoreEditorsOutputFiles->'%(RootDir)%(Directory)%(Filename).*')">
      <Output TaskParameter="Include" ItemName="CoreEditorsOutputFiles" />
    </CreateItem>

    <Copy SourceFiles="@(CoreOutputFiles)" DestinationFiles="@(CoreOutputFiles->'$(CoreDestFolder)%(Filename)%(Extension)')" />
    <Copy SourceFiles="@(PluginsOutputFiles)" DestinationFiles="@(PluginsOutputFiles->'$(PluginsDestFolder)%(Filename)%(Extension)')" />
	  <Copy SourceFiles="@(CorePluginsOutputFiles)" DestinationFiles="@(CorePluginsOutputFiles->'$(CorePluginsDestFolder)%(Filename)%(Extension)')" />
	  <Copy SourceFiles="@(CoreEditorsOutputFiles)" DestinationFiles="@(CoreEditorsOutputFiles->'$(CoreEditorsDestFolder)%(Filename)%(Extension)')" />
    <!-- Copy everything in Dependencies -->
    <ItemGroup>
      <EditorDependencies Include="..\..\src\nodes\editors\**\Dependencies\*" />
      <PluginDependencies Include="nodes\plugins\**\Dependencies\*" />
	  <CorePluginDependencies Include="..\..\src\nodes\plugins\**\Dependencies\*" />
      <PluginLicenses Include="nodes\plugins\**\license*.txt" />
      <PluginGirlpowers Include="nodes\plugins\**\girlpower\*" />
    </ItemGroup>
  	<Copy SourceFiles="@(EditorDependencies)" DestinationFiles="@(EditorDependencies->'$(CoreEditorsDestFolder)%(Filename)%(Extension)')" />
    <Copy SourceFiles="@(PluginDependencies)" DestinationFiles="@(PluginDependencies->'$(PluginsDestFolder)%(Filename)%(Extension)')" Condition="!Exists('%(PluginDependencies.RootDir)%(PluginDependencies.Directory)..\ignore')" />
	  <Copy SourceFiles="@(CorePluginDependencies)" DestinationFiles="@(CorePluginDependencies->'$(CorePluginsDestFolder)%(Filename)%(Extension)')" Condition="!Exists('%(PluginDependencies.RootDir)%(PluginDependencies.Directory)..\ignore')" />	
    <Copy SourceFiles="@(PluginLicenses)" DestinationFiles="@(PluginLicenses->'$(PluginsDestFolder)..\..\..\licenses\%(Filename)%(Extension)')" Condition="!Exists('%(PluginLicenses.RootDir)%(PluginLicenses.Directory)ignore')" />
  </Target>
  <Target Name="Rebuild" DependsOnTargets="$(RebuildDependsOn)" />
  <!--
  This target can be used to clean all of the projects before you build them.
  It will also delete any dll & exe files located in the the Deploy folder. This could be accomplished
  much better than this, but this is quick and easy.
  -->
  <Target Name="Clean">
    <!-- Delete any files this process may have created from a previous execution -->
    <CreateItem Include="$(CoreDestFolder)\**\*exe;$(CoreDestFolder)\**\*dll;$(CoreDestFolder)\**\*xml;$(CoreDestFolder)\**\*pdb;$(PluginsDestFolder)\**\*exe;$(PluginsDestFolder)\**\*dll;$(PluginsDestFolder)\**\*xml;$(PluginsDestFolder)\**\*pdb;$(CorePluginsDestFolder)\**\*exe;$(CorePluginsDestFolder)\**\*dll;$(CorePluginsDestFolder)\**\*xml;$(CorePluginsDestFolder)\**\*pdb;$(CoreEditorsDestFolder)\**\*exe;$(CoreEditorsDestFolder)\**\*dll;$(CoreEditorsDestFolder)\**\*xml;$(CoreEditorsDestFolder)\**\*pdb">
      <Output ItemName="GeneratedFiles" TaskParameter="Include" />
    </CreateItem>
    <Delete Files="@(GeneratedFiles)" />
    <MSBuild Projects="@(CoreProjectsToBuild);@(PluginProjectsToBuild);@(CorePluginsToBuild);@(CoreEditorsToBuild" Targets="Clean" Properties="Configuration=$(Configuration);" />
  </Target>
