   <PIN pinname="Input String" visible="0" slicecount="1" values="|float FarPlane;&cr;&lf;&cr;&lf;//get depth only&cr;&lf;float GetDepth(float4 col)&cr;&lf;{&cr;&lf;  return (col.b + col.a / 255) * FarPlane;&cr;&lf;}&cr;&lf;&cr;&lf;//get normals only&cr;&lf;float3 GetNormals(float4 col)&cr;&lf;{&cr;&lf;  float3 n;&cr;&lf;  n.xy = col.rg * 2 - 1;&cr;&lf;  n.z = -sqrt(1 - dot(n.xy, n.xy));&cr;&lf;  return n;&cr;&lf;}&cr;&lf;&cr;&lf;//get normals and depth&cr;&lf;float4 GetNormalsAndDepth(float4 col)&cr;&lf;{&cr;&lf;  float4 n;&cr;&lf;  n.xy = col.rg * 2 - 1;&cr;&lf;  n.z = -sqrt(1 - dot(n.xy, n.xy));&cr;&lf;  n.w = (col.b + col.a / 255) * FarPlane;&cr;&lf;  return n;&cr;&lf;}|">
   </PIN>
