<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- Hosting needs to be intalled in GAC for AnyCPU to work -->
  <ItemGroup Condition="'$(Platform)' == 'AnyCPU'">
    <Reference Include="VVVV.Hosting" />
  </ItemGroup>
  <ItemGroup Condition="'$(Platform)' == 'x86'">
    <Reference Include="VVVV.Hosting">
      <HintPath>$(MSBuildThisFileDirectory)\lib\x86\VVVV.Hosting.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup Condition="'$(Platform)' == 'x64'">
    <Reference Include="VVVV.Hosting">
      <HintPath>$(MSBuildThisFileDirectory)\lib\x64\VVVV.Hosting.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>